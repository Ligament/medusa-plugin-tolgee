{
  "name": "medusa-plugin-tolgee",
  "description": "Medusa V2 Plugin to easily add translations to your models with Tolgee ",
  "author": "SteelRazor47",
  "version": "2.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SteelRazor47/medusa-plugin-tolgee"
  },
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    ".": "./.medusa/server/src/modules/tolgee/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa-plugin-integration",
    "translation",
    "tolgee",
    "ecommerce",
    "medusa-plugin-cms",
    "medusa-v2",
    "medusa"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:workflows": "TEST_TYPE=integration:workflows NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.12.1",
    "@medusajs/admin-shared": "2.12.1",
    "@medusajs/cli": "2.12.1",
    "@medusajs/dashboard": "^2.12.1",
    "@medusajs/framework": "2.12.1",
    "@medusajs/icons": "2.12.1",
    "@medusajs/js-sdk": "2.12.1",
    "@medusajs/medusa": "2.12.1",
    "@medusajs/test-utils": "2.12.1",
    "@medusajs/ui": "4.0.29",
    "@medusajs/utils": "2.12.1",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "jest": "^30.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "yalc": "1.0.0-pre.53",
    "zod": "4.1.13"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.11.2",
    "@medusajs/cli": "^2.11.2",
    "@medusajs/framework": "^2.11.2",
    "@medusajs/icons": "^2.11.2",
    "@medusajs/medusa": "^2.11.2",
    "@medusajs/test-utils": "^2.11.2",
    "@medusajs/ui": "^4.0.26",
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.13.2",
    "react-i18next": "^13.5.0",
    "react-router-dom": "6.20.1"
  },
  "dependencies": {
    "@tolgee/core": "^6.2.7",
    "@tolgee/react": "^6.2.7",
    "@tolgee/web": "^6.2.7",
    "axios-cache-interceptor": "^1.9.0",
    "bottleneck": "^2.19.5",
    "dataloader": "^2.2.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.20.0"
}